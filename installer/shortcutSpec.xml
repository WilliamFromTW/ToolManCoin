<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="KafeiouCoin" location="startMenu"/>
	<shortcut
		name="KafeiouCoin Server"
		target="$INSTALL_PATH\KafeiouCoin.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start KafeiouCoin server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
